import java.util.Scanner;
public class Class1 
{
	public static void main(String [] args)
	{
		Scanner input = new Scanner (System.in);
		
		System.out.print("Type in a number ");
		int num = input.nextInt();
		for (int i = 1; i <= 10; i++);
			System.out.println(num + " * " + i + " = " + (num*i));
	}
}
